<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/alivc_superclass_radius_2"/>
            <solid android:color="@color/alivc_superclass_color_btn_unenable_blue"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/alivc_superclass_radius_2"/>
            <solid android:color="@color/alivc_superclass_color_btn_enable_blue"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/alivc_superclass_radius_2"/>
            <solid android:color="@color/alivc_superclass_color_btn_unenable_blue"/>
        </shape>
    </item>
</selector>